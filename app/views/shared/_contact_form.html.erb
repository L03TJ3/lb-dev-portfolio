<form id="contact-me" class="col s12 contact-form">
  <div class="row">
    <div class="input-field col s6">
      <i class="material-icons md-light prefix">account_circle</i>
      <input id="icon_prefix" type="text"
             class="validate">
      <label for="icon_prefix">First Name</label>
    </div>
    <div class="input-field col s6">
      <input id="last_name" type="text" class="validate">
      <label for="last_name">Last Name</label>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12">
      <i class="material-icons md-light prefix">contact_mail</i>
      <input id="icon_prefix1" type="email" class="validate">
      <label for="icon_prefix1">Email</label>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12">
      <i class="material-icons md-light prefix">mode_edit</i>
      <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
      <label for="icon_prefix2">Message</label>
    </div>
  </div>
  <button class="btn waves-effect waves-light right send" type=""
  name="">Submit
    <i class="material-icons right">send</i>
  </button>
</form>
